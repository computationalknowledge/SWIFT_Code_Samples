class Pizza
{
    var size:Int        = 5
    var thickness:Int   = 2
    var type:String     = ""
}

var pizza:Pizza = Pizza()
pizza.type = "Pepperoni"

print("Made a \(pizza.type) pizza with size \(pizza.size)")

